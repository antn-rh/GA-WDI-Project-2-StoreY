<div class="col-md-6 col-md-offset-4">
  <h1>Sign In</h1>

  <%= form_for :login do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.submit 'Sign In' %>
  </div>
  <% end %>

  <br>
  Don't have an account? Sign up <%= link_to 'here', new_user_path %>
</div>
