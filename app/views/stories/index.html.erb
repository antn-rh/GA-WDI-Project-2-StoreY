<div class="row profileLinks">
  <%= link_to 'back to profile', user_path(@user) %>
</div>

<h1> <%= @user.name %> - archive </h1>

<ul>
  <% @stories.each do |story| %>
    <li>
      <%= link_to story.title, user_story_pins_path(@user, story, story.pins) %>
      <% story.pins.each do |pin| %>
        <%= link_to (image_tag pin.image.url(:large)), user_story_pins_path(@user, story, story.pins) if pin.image? %>
      <% end %>
    </li>
  <% end %>
</ul>
