<div class="row profileLinks">
  <%= link_to 'back to profile', user_path(@user) %>
</div>

<h1>Stories</h1>

<ul>
  <% @stories.each do |story| %>
    <li>
      <%= link_to story.title, user_story_path(@user, story) %>
      <% story.pins.each do |pin| %>
        <%= image_tag pin.image.url(:med) if pin.image? %>
      <% end %>
    </li>
  <% end %>
</ul>
