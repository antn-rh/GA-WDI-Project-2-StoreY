<div class="row profileLinks">
  <%= link_to 'back to profile', user_path(@user) %>
  <br>
  <%= link_to 'back to story archive', user_stories_path(@user) %>
</div>

<div class="col-md-6 col-md-offset-3">
  <h1><%= @story.title %> - archive </h1>
  <h3> <%= @story.description %> </h3>
  <h3> <%= @story.location %> </h3>
</div>

<div class="transitions-enabled" id="pins">
  <% @pins.each do |pin| %>
  <div class="box panel panel-default">
    <%= image_tag pin.image.url() if pin.image? %>
    <div class="panel-body">
      <%= pin.text %>
    </div>
  </div>
  <% end %>
</div>
