<h1>Users</h1>

<ul>
  <% @users.each do |user| %>
    <li>
      <%= user.name %>
    </li>
  <% end %>
  <br>

  <% if current_user %>
    Logged in as: <%= current_user.name %>
    <br>
    <%= link_to 'Log Out', logout_path, method: :delete %>
  <% else %>
    <%= link_to 'Log In', login_path %>
    <br>
    <%= link_to 'Sign Up', new_user_path %>
  <% end %>
</ul>
