<h1>Home</h1>
<ul>
  <% @users.each do |user| %>
    <li>
      <%= user.name %> - <%= link_to 'Show', user_path(user) %>
    </li>
  <% end %>
  <br>

  <% if current_user %>
    Logged in as: <%= current_user.name %>
    <br>
    <%= link_to 'Sign Out', logout_path, method: :delete %>
  <% else %>
    <%= link_to 'Sign In', login_path %>
    <br>
    <%= link_to 'Sign Up', new_user_path %>
  <% end %>
</ul>
